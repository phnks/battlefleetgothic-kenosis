                  <profiles>
                    <profile id="6a53bc82-3ec8-45a9-a7b2-de68ce50f72a" name="6. Modulate Wavelengths" hidden="false" typeId="6565b064-bc0f-453e-8601-43cadef4b8dc" typeName="Special Rule">
